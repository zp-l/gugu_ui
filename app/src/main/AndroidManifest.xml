        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Light.NoTitleBar">
        <activity android:name="com.qibei.gugu.MainActivity"
                  android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--登录 -->
        <activity android:name="com.qibei.gugu.activity.LoginActivity"></activity>
        <!--注册 -->
        <activity android:name="com.qibei.gugu.activity.RegisterActivity"></activity>
        <!--找医院 -->
        <activity android:name="com.qibei.gugu.activity.FindHospitalActivity"></activity>
