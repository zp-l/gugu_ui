            android:theme="@style/Transparent"></activity>
        <!--个人信息 -->
        <activity android:name="com.qibei.gugu.activity.UserInfoActivity"></activity>
        <!--关于呱呱 -->
        <activity android:name="com.qibei.gugu.activity.AboutActivity"></activity>
        <!--账号与安全 -->
        <activity android:name="com.qibei.gugu.activity.AccountSecurityActivity"></activity>
        <!--服务条款 -->
        <activity android:name="com.qibei.gugu.activity.ServiceProvisionActivity"></activity>
        <!--代金卷 -->
        <activity android:name="com.qibei.gugu.activity.VoucherActivity"></activity>
        <!--我的银行卡 -->
        <activity android:name="com.qibei.gugu.activity.MyBankCardActivity"></activity>
        <!--支付 -->
        <activity android:name="com.qibei.gugu.activity.PayActivity"></activity>

        <!--支付 -->
        <activity android:name="com.qibei.gugu.activity.PayReturnActivity"></activity>

        <!--查百科 -->
        <activity android:name="com.qibei.gugu.activity.FindEncyclopediasActivity"></activity>

        <!--查百科 -->
        <activity android:name="com.qibei.gugu.activity.EncyclopediasDetailActivity"></activity>

        <activity android:name="com.qibei.gugu.sample.StaggeredGridActivityFragment"/>
        <activity android:name="com.qibei.gugu.sample.StaggeredGridEmptyViewActivity" />
        <activity android:name="com.qibei.gugu.sample.ListViewActivity"/>

        <!--产品详情 -->
        <activity android:name="com.qibei.gugu.activity.ProductDetailsActivity"></activity>

