        <activity android:name="com.qibei.gugu.activity.OrderModifyActivity"></activity>
        <!--我的收藏 -->
        <activity android:name="com.qibei.gugu.activity.MyCollectionActivity"></activity>
        <!--我的订单 -->
        <activity android:name="com.qibei.gugu.activity.MyOrderActivity"></activity>
        <!--订单详情 -->
        <activity android:name="com.qibei.gugu.activity.OrderDetailsActivity"></activity>
        <!--测试 -->
        <activity android:name="com.qibei.gugu.activity.TestActivity"></activity>
        <!--咨询 -->
        <activity
